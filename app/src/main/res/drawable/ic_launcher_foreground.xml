<?xml version="1.0" encoding="utf-8"?>
<!--
    Adaptive icon foreground: pin from logo.png
    Viewport: 108x108dp (standard adaptive icon size)
    Safe zone: center 72x72dp (18dp inset on each side)
    Pin is drawn within the safe zone, centered at (54,54)
-->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!--
        Teardrop pin with even-odd hole.
        Pin center X = 54, pin top Y = 20, pin bottom (tip) Y = 88
        Head circle: center (54, 44), radius 24
        Hole circle: center (54, 43), radius 9.5
    -->
    <path
        android:fillColor="#3C3734"
        android:fillType="evenOdd"
        android:pathData="
            M 54,88
            C 53.1,87.1 30,62 30,44
            A 24,24 0 1 1 78,44
            C 78,62 54.9,87.1 54,88
            Z
            M 54,34.5
            A 9.5,9.5 0 1 0 54,53.5
            A 9.5,9.5 0 1 0 54,34.5
            Z
        " />
</vector>
